<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.elisoft.pedidosmontero">


    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PedidosMontero">
        <activity android:name=".Animacion">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Informacion"
            android:label="@string/title_activity_informacion"
            android:theme="@style/Theme.PedidosMontero" />
        <activity
            android:name=".Peril_usuario"
            android:label="@string/title_activity_peril_usuario"
            android:theme="@style/Theme.PedidosMontero" />
        <activity
            android:name=".Perfil_conductor"
            android:label="@string/title_activity_perfil_conductor"
            android:theme="@style/Theme.PedidosMontero" />
        <activity
            android:name=".Iniciar_sesion"
            android:label="@string/title_activity_iniciar_sesion"
            android:theme="@style/Theme.PedidosMontero" />
        <activity
            android:name=".Registro"
            android:label="@string/title_activity_registro"
            android:theme="@style/Theme.PedidosMontero" />
        <activity
            android:name=".Principal"
            android:label="@string/title_activity_principal"
            android:theme="@style/Theme.PedidosMontero">

        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>